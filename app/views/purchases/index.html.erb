<h1>Bought</h1>

<% @purchases.each do |purchase| %>
   <h2 class="mt-3"> <%= purchase.date_purchased.strftime("%Y %m %d") %> </h2>
   <h6> <%= purchase.car.make %> <%= purchase.car.model %> from <%= purchase.seller.first_name %> </h6>
<% end %>

<h1 class="mt-3">Sold</h1>

<% @sales.each do |sale| %>
    <%= sales.date_purchased %>
<% end %>